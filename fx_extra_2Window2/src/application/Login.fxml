<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="86.0" prefWidth="339.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="application.LoginController">
   <center>
      <TextField fx:id="loginTextField" BorderPane.alignment="CENTER" />
   </center>
   <right>
      <Button onAction="#loginAction" mnemonicParsing="false" text="go" BorderPane.alignment="CENTER" />
   </right>
   <top>
      <Label text="Login" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
