package app;

public class Methoden {
	// soll die grossere von zwei Zahlen zuruck geben
	public static int max(int a, int b) {
		if (a>b) {
			return a;
		}
		return b;
	}
	
	// soll die großte von drei Zahlen zuruck geben
	public static int max(int a, int b, int c) {
		return 0;
	}
	
	// soll die großte Zahl eines Arrays zuruck geben
	public static int max(int[] arr) {
		return 0;
	}
	
	// soll nur gerade Zahlen ausgeben
	public static void printGerade(int[] arr) {}
	
	/**
	 *Zusatz
	 *soll ein zweidimensionales Array in dieser Form ausgeben
	 * 1 2 3 2
	 * 3 5 5 6
	 * 2 4 6 7
	 * 
	 */
	
	public static void print2d(int[][] arr2d) {}

	
	public static void main(String[] args) {
		int[][] arr2d = 	{
				{3,4},
				{7,5},
				{7,5}
			};
//		print2D(arr2d);
		
		
		System.out.println(max(4,2));
	}

}
