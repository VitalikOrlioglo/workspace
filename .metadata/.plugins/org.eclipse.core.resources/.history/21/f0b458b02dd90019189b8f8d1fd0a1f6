package app;

import java.io.Serializable;

public class Auto implements Fahrzeug, Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	@Override
	public void fahre() {
		System.out.println("Auto fahrt . . . ");
	}

	@Override
	public void stop() {
		System.out.println("Auto stop . . . ");

	}
	
}
