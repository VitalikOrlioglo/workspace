package app;

public class Main {

	public static void main(String[] args) {
		int num1 = 0;
		int num2 = 0;
		char op = '+';
		double res = Rechner.rechner(5, 0, '/');
		
		if (Double.isNaN(res)) {
			System.out.println("Fehler");
		}else {
			System.out.println("Ergebnis: " + num1 + op + num2 + " = " + res);
		}
	}

}
